{
  "name": "AdvTesting",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "rndebugger-open",
    "compile": "tsc --noEmit -p .",
    "precommit": "lint-staged",
    "prepush": "yarn test",
    "format": "yarn -s format:js && yarn -s format:ts && yarn -s format:json",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:ts": "prettier --write **/*.{ts,tsx} && tslint --fix -p tsconfig.json",
    "lint": "yarn -s lint:ts",
    "lint:ts": "tslint -p tsconfig.json",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "storybook": "storybook start -p 9001 -c .storybook --skip-packager",
    "test": "jest"
  },
  "dependencies": {
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mobx-state-tree": "^1.3.1",
    "ramda": "^0.25.0",
    "react": "16.2.0",
    "react-native": "0.52.1",
    "react-native-animatable": "^1.2.4",
    "react-native-i18n": "^2.0.10",
    "react-navigation": "^1.0.0-beta.27",
    "solidarity": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/ramda": "^0.25.16",
    "@types/react": "^16.0.34",
    "@types/react-native": "^0.52.2",
    "@types/react-navigation": "^1.0.28",
    "@types/validate.js": "^0.11.0",
    "babel-jest": "22.1.0",
    "babel-plugin-transform-inline-environment-variables": "^0.2.0",
    "babel-preset-react-native": "4.0.0",
    "husky": "^0.14.3",
    "jest": "22.1.4",
    "lint-staged": "^6.0.1",
    "prettier": "^1.10.2",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.2.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "validate.js": "^0.12.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "concurrent": false,
    "subTaskConcurrency": 1,
    "linters": {
      "*.{ts,tsx}": [
        "prettier --write",
        "tslint --fix -p tsconfig.json",
        "git add"
      ]
    }
  }
}
